{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff2?|ttf|eot|json|webmanifest)$).*)",
      "destination": "/index.html"
    }
  ]
}
