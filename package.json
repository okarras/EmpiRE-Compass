{
  "name": "empire-compass-dashboard",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:build": "npm run build-storybook",
    "storybook:serve": "npx http-server storybook-static -p 8080",
    "deploy:chromatic": "chromatic --exit-zero-on-changes",
    "deploy:firebase": "npm run build-storybook && firebase deploy --only hosting",
    "deploy:vercel": "npm run build-storybook && vercel --prod",
    "deploy:surge": "npm run build-storybook && surge storybook-static",
    "preview:storybook": "npm run build-storybook && npm run storybook:serve",
    "prepare": "husky",
    "release": "standard-version"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0-alpha.3",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^2.0.0-alpha.3",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^6.4.10",
    "@mui/material": "^6.4.11",
    "@mui/x-charts": "^7.24.1",
    "@mui/x-data-grid": "^8.1.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/dompurify": "^3.0.5",
    "ai": "^4.3.16",
    "dompurify": "^3.2.6",
    "firebase": "^11.6.0",
    "html-to-image": "1.11.13",
    "prettier-eslint": "^16.3.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.5.0",
    "react-router-dom": "^7.5.0",
    "reactflow": "11.11.4",
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-onboarding": "^9.0.4",
    "@storybook/addon-vitest": "^9.0.14",
    "@storybook/react-vite": "^9.0.4",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.2.0",
    "@vitest/coverage-v8": "^3.2.0",
    "chromatic": "^13.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^9.0.4",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "playwright": "^1.52.0",
    "prettier": "^3.4.2",
